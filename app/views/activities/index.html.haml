- content_for :title do
  = t(".title")
- content_for :action_button do
  = icon_link_to new_team_path, "file_download", 
    class: "action-button waves-effect"
- content_for :notice do
  = notice
- content_for :breadcrumbs do
  = render_breadcrumbs
  
%table
  %thead
    %tr
      %th= t(".name")
      %th= t(".plan_description")
      %th= t(".execution_report")

  %tbody
    - @activities.each do |activity|
      %tr
        %td= activity.name
        %td= activity.description
        %td= activity.execution_report_content
